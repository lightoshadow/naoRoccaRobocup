                  <![CDATA[import socket
import time

class MyClass(GeneratedClass):
    global socket_ny

    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.socket_ny = None

    def onLoad(self):
        pass

    def onUnload(self):
        pass

    def onInput_start_server(self):
        self.logger.info("Connessione ...")
        self.connecting()

        # setup socket
        self.socket_ny = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        self.socket_ny.setsockopt(socket.SOL_SOCKET, socket.SO_REUSEADDR, 1)
        self.socket_ny.connect((self.getParameter("IP Address"), self.getParameter("Port")))
        #self.socket_ny.connect(("192.168.203.63", 8080))
        self.logger.info("Connesso! " + self.getParameter("IP Address"))

        # send some data
        self.socket_ny.send("ciao sono grigio!".encode("utf-8"))
        time.sleep(5)
        self.connected()

        # loop
        while True:
            val = self.socket_ny.recv(2048)
            self.logger.info(val)
            #self.logger.info(val.decode("utf-8"))
            if "dialogo 2b" in val:
                self.start_dialog2()
            elif "Arduino" in val:
                self.hello_arduino()
            time.sleep(1)

    def onInput_finished_connecting(self):
        self.logger.info(self.socket_ny)
        self.logger.info("connesso")

    def onInput_dialog1(self):
        self.socket_ny.send("dialogo 1e".encode("utf-8"))

    def onInput_dialog2(self):
        self.socket_ny.send("dialogo 2e".encode("utf-8"))

    def onInput_onStop(self):
        self.onUnload()
        pass]]>
